{
    "name": "alshaya/spc-middleware",
    "type": "project",
    "description": "Provides a middleware for the Alshaya spc.",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "springimport/magento2-api-v1": "^1.0",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/lock": "4.3.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {"php": "7.4"},
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Drupal\\Component\\": "../core/lib/Drupal/Component",
            "Drupal\\alshaya_spc\\": "../modules/react/alshaya_spc/src",
            "Drupal\\alshaya_acm_customer\\": "../modules/custom/alshaya_acm_customer/src",
            "Drupal\\alshaya_master\\": "../modules/custom/alshaya_master/src",
            "Drupal\\alshaya_user\\": "../modules/custom/alshaya_user/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
