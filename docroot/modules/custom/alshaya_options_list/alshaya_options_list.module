<?php

/**
 * @file
 * Module file for alshaya options list.
 */

/**
 * Implements hook_theme().
 */
function alshaya_options_list_theme($existing, $type, $theme, $path) {
  $items = [];

  $items['alshaya_options_page'] = [
    'render element' => 'form',
    'variables' => [
      'options_list' => NULL,
      'page_title' => '',
      'description' => '',
    ],
  ];

  $items['alshaya_options_attribute'] = [
    'variables' => [
      'option' => NULL,
      'attribute_code' => NULL,
    ],
  ];

  return $items;
}
