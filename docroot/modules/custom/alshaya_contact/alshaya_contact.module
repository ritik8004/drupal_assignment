<?php

/**
 * @file
 * Module file.
 */

use Drupal\Core\Entity\EntityInterface;

/**
 * Implements hook_ENTITY_TYPE_presave().
 */
function alshaya_contact_webform_submission_presave(EntityInterface $entity) {
  if ($entity->bundle() == 'alshaya_contact') {
    $data = $entity->getData();
    $data['mobile_number'] = $data['mobile_number']['value'];
    $entity->setData($data);
  }
}
