<div>
    <table>
        <thead>
            <tr>
                <th>{{ 'Product' | t }}</th>
                <th>{{ 'Unit price' | t }}</th>
                <th>{{ 'Available action' | t }}</th>
            </tr>
        </thead>
        <tbody>
            {% if data %}
                {% for item in data %}
                    <tr>
                        <td>
                            <div>{{ item.image }} fdsfd</div>
                            <div>{{ item.name }} dfsd </div>
                            {% for sku_attribute in sku_attributes %}
                                <div>{{ sku_attribute.label }}: {{ sku_attribute.value }}</div>
                            {% endfor %}
                            <div>{{ item.item_code }}</div>
                        </td>
                        <td>
                            <div>{{ item.price }}</div>
                        </td>
                        <td>
                            <div>{{ 'Add to Wishlist' | t }}</div>
                        </td>
                    </tr>
                {% endfor %}
            {% endif %}
        </tbody>
    </table>
</div>