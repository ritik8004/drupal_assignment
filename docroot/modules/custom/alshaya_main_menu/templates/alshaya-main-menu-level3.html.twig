<ul class="menu__list menu--three__list">
  <div class="level-three__wrapper">
    <span class="back--link"><span class="menu__back"></span> <span>{{ data.label }}</span></span>
    {% for key, level3 in data.child %}
      <li class="menu__list-item menu--three__list-item {{ level3.class|join(' ') }}">
        <div class="menu__link-wrapper menu-two__link-wrapper">
          <{{ level3.tag }} class="menu__link menu--three__link {{ level3.class|join(' ') }} {{ data.sub-nav-class }}" gtm-menu-title="{{ level3.gtm_menu_title }}" {{ level3.tag_attr }}>{{ level3.label }}</{{ level3.tag }}>
        </div>

        {% if level3.child is not empty %}
          {{ alshaya_main_menu('alshaya_main_menu_level4', level3) }}
        {% endif %}
      </li>
    {% endfor %}
  </div>
</ul>
