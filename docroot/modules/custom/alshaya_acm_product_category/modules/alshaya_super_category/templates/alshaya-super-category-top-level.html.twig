<div class="main--menu">
  <ul class="menu__list menu--one__list">
    {% for key1, level1 in term_tree %}
      {% if level1.class == true %}
      <li class="menu__list-item menu--one__list-item {{ level1.class }}">
      {% else %}
        <li class="menu__list-item menu--one__list-item">
      {% endif %}
        <div class="menu__link-wrapper menu-two__link-wrapper">
          {% if level1.imgPath is not null and level1.imgPath is not empty %}
            <a class="menu__link menu--one__link {{ level1.class }}" href="{{ level1.path }}" title="{{ level1.meta_title }}" gtm-menu-title="{{ level1.gtm_menu_title }}" data-super-category-label="{{ level1.label }} ">
              <img class="image-container" data-hover-image="{{ level1.inactive_path }}" data-org-image="{{ level1.imgPath }}" src="{{ level1.imgPath }}" alt="{{ level1.label }}">
            </a>
          {% endif %}
      </div>
      </li>
    {% endfor %}
  </ul>
</div>
