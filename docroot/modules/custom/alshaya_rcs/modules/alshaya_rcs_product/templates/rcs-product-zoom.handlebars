<div id="product-zoom-container">
  {{!-- {% block defaultGallery %} --}}
    <div class="cloudzoom thumbnail-count-{{ thumbnails.length }}">
    <div class="cloudzoom__herocontainer">
      <div id="cloud-zoom-wrap" class="cloudzoom__image">
        {{!-- @todo Check what is the main image --}}
        <div class="img-wrap" data-scale="2"><img data-zoom-url="{{ mainImage.zoomurl }}" loading="lazy" src="{{ mainImage.mediumurl }}" alt="{{ mainImage.label }}" title="{{ mainImage.label }}" /></div>
      </div>
      <div class="yt-vi-container cloudzoom__video_main"></div>
      {{#if (gt labels.length 0) }}
        <div class="product-labels">
          {{ labels }}
        </div>
      {{/if}}
    </div>
    <div class="cloudzoom__thumbnails">
      <ul id="lightSlider" class="{{ pager_flag }}">
      {{#each thumbnails}}
        {{#if (eq this.type 'youtube')}}
          <li data-iframe="{{ this.url }}" class="cloudzoom__thumbnails__video youtube">
            <img width='{{ this.width }}' loading="lazy" height='{{ this.height }}' src="{{ this.thumburl }}" alt="{{ this.video_desc }}" title="{{ this.video_title }}" />
          </li>
        {{else if (eq this.type 'vimeo')}}
          <li data-iframe="{{ this.url }}" class="cloudzoom__thumbnails__video vimeo">
            <img width='{{ this.width }}' loading="lazy" height='{{ this.height }}' src="{{ this.thumburl }}" alt="{{ this.video_desc }}" title="{{ this.video_title }}" />
          </li>
        {{else if (eq this.type 'video')}}
          <li class="cloudzoom__thumbnails__video pdp-video">
            <button class="pdp-video-close"></button>
            <video class="gallery-video b-lazy" muted="muted" loop>
              <source src="{{ lazy_load_placeholder }}" data-src="{{ this.url }}">
              {{t 'Your browser does not support the video tag.'}}
            </video>
            <div class="pdp-video-controls">
              <div class="control" title="{{t 'Play/Pause Video'}}"></div>
            </div>
          </li>
        {{ else }}
          <li>
            <a data-zoom-url="{{ this.zoomurl }}" href="{{ this.mediumurl }}" class="cloud-zoom-gallery cloudzoom__thumbnails__image">
              <img loading="lazy" src="{{ this.thumburl }}" alt="{{ this.label }}" title="{{ this.label }}" />
            </a>
          </li>
        {{/if}}
      {{/each}}
      </ul>
    </div>
  </div>
  {{!-- {% endblock %} --}}
  {{!-- {% block mobileGallery %} --}}
    {{#if (eq pdp_gallery_type 'classic')}}
      {{!-- Add slider counter --}}
      <div class="slider__counter"></div>
    {{/if}}
    <div class="mobilegallery">
      <ul id="product-image-gallery-mobile" class="mobilegallery__thumbnails {{ pager_flag }}">
        {{#each thumbnails}}
          {{#if (eq thumbs.type 'youtube')}}
            <li class="mobilegallery__thumbnails__video youtube">
              <div class="mobileplayer-wrapper">
                <iframe class="mobileplayer" src={{ thumbs.url }} style="position:absolute;top:0;left:0;width:100%;height:100%;" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
              </div>
            </li>
          {{else if (eq thumbs.type 'vimeo')}}
            <li class="mobilegallery__thumbnails__video vimeo">
              <div class="mobileplayer-wrapper">
                <iframe class="mobileplayer" src={{ thumbs.url }} style="position:absolute;top:0;left:0;width:100%;height:100%;" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
              </div>
            </li>
          {{else if (eq thumbs.type 'video')}}
            <li class="mobilegallery__thumbnails__video pdp-video">
              <button class="pdp-video-close"></button>
              <video class="gallery-video b-lazy" muted="muted" loop>
                <source src="{{ lazy_load_placeholder }}" data-src="{{ thumbs.url }}">
                {{t 'Your browser does not support the video tag.'}}
              </video>
              <div class="pdp-video-controls">
                <div class="control" title="{{t 'Play/Pause Video'}}"></div>
              </div>
            </li>
          {{else}}
            <li>
              <img loading="lazy" src="{{ thumbs.zoomurl }}" alt="{{ thumbs.label }}" title="{{ thumbs.label }}" />
            </li>
          {{/if}}
        {{/each}}
      </ul>
    {{#if (gt labels.length 0)}}
      {{!-- Product lozenges --}}
      <div class="product-labels">
        {{ labels }}
      </div>
    {{/if}}
  </div>
  {{!-- {% endblock %} --}}
  {{!-- Add swatch images as magazine --}}
  {{#if (eq pdp_gallery_type 'classic')}}
    {{!-- {% block magazineGallery %} --}}
      <div class="magazine__gallery--container">
        <div class="magazine-product-description">
          <div class="magazine-swatch-placeholder"></div>
        </div>
      </div>
    {{!-- {% endblock %} --}}
  {{/if}}
  {{!-- {% block modalGallery %} --}}
    <div id="product-full-screen-gallery-container" style="display: none;">
      <div class="imagegallery__wrapper thumbnail-count-{{ thumbnails.length }}">
        <div class="yt-vi-container cloudzoom__video_modal"></div>
        <ul id="product-full-screen-gallery">
        {{!-- As per new desgn we dont show videos in full screen modal galleries. --}}
          {{#each thumbnails}}
            {{#if (ne this.type 'youtube')}}
              {{#if (ne this.type 'vimeo')}}
                {{#if (ne this.type 'video')}}
                  <li>
                    <a href="{{ this.fullurl }}" class="imagegallery__thumbnails__image a-gallery">
                      <img loading="lazy" src="{{ this.fullurl }}" alt="{{ this.label }}" title="{{ this.label }}" />
                    </a>
                  </li>
                {{/if}}
              {{/if}}
            {{/if}}
          {{/each}}
        </ul>
      </div>
    </div>
  {{!-- {% endblock %} --}}
</div>
