<div id="pdp-home-delivery" class="delivery-options delivery-options-wrapper">
  <div class="c-accordion-delivery-options ui-accordion ui-widget ui-helper-reset" role="tablist">
    <div class="express-delivery-title-wrapper title ui-accordion-header ui-corner-top ui-state-default ui-accordion-icons ui-accordion-header-active ui-state-active" role="tab" id="ui-id-1" aria-controls="ui-id-2" aria-selected="false" aria-expanded="false" tabindex="0">
      <span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span>
      <h3 class="c-accordion__title express-delivery-title">
        {{#if title }}
          {{ title }}
          <span class="subtitle express-delivery-subtitle">{{ subtitle }}</span>
        {{/if}}
      </h3>
    </div>
    {{#if (eq type 'express_delivery') }}
      <div class="c-accordion_content ui-accordion-content ui-corner-bottom ui-helper-reset ui-widget-content ui-accordion-content-active" id="ui-id-2" aria-labelledby="ui-id-1" role="tabpanel" aria-hidden="false" style="">
        <div id="express-delivery-options"></div>
      </div>
    {{else if (eq type 'standard_delivery') }}
      <div class="c-accordion_content">
        <div class="delivery-option-content">
          <div class="delivery-option-title">
            {{#if standard_title }}
              {{ standard_title }}
            {{/if}}
            {{#if standard_subtitle }}
            <span>{{ standard_subtitle }}</span>
            {{/if}}
          </div>
        </div>
      </div>
    {{/if}}
  </div>
</div>
