<div class="visual-mobile-collection-wrapper">
  {{#each menu_items as |l3_item|}}
    <{{@l3_item.tag}} class="visual-mobile-level-three__link" {{{l3_item.tag_attr}}}>
      <img class="visual-mobile-level_three__image" src="{{l3_item.image}}">
      <div class="visual-mobile-level-three__heading">{{l3_item.name}}</div>
    </{{@l3_item.tag}}>
  {{/each}}
</div>
