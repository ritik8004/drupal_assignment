{% if concepts is not empty %}
  <div class="concept-wrapper">
    <div class="concept-label">{{ title }}</div>
    <div class="concept-value">
      <ul>
        {% for key,concept in concepts %}
          {% if concept is not empty %}
            <li>{{ concept.value }}</li>
          {% endif %}
        {% endfor %}
      </ul>
    </div>
  </div>
{% endif %}
