{% if properties is not empty %}
  <div class="main-attribute-wrapper">
    <div class="main-attribute-label">{{ properties.title }}</div>
    <div class="main-attribute-value">
      <ul>
        {% for key,property in properties.value %}
          {% if property is not empty %}
            <li>{{ property.value }}</li>
          {% endif %}
        {% endfor %}
      </ul>
    </div>
  </div>
{% endif %}
