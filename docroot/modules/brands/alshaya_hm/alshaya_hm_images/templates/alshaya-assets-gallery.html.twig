<div class="alshaya_search_gallery">
	{% if mainImage %}
		<div class="alshaya_search_mainimage">
			<img src="{{ mainImage.url }}" alt="{{ label }}" title="{{ label }}">
		</div>
	{% endif %}
	{% if hoverImage %}
		<div class="alshaya_search_hoverimage">
			<img src="{{ hoverImage.url }}" alt="{{ label }}" title="{{ label }}">
		</div>
	{% endif %}
</div>