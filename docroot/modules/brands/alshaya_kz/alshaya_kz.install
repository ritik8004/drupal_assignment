<?php

/**
 * @file
 * Install, uninstall, update function for alshaya_kz.
 */

/**
 * Implements hook_install().
 */
function alshaya_kz_install() {
  // Enable and set alshaya_kidzania as default theme.
  \Drupal::service('theme_handler')->install(['kidzania'], TRUE);
  \Drupal::service('theme_handler')->setDefault('kidzania');
}
