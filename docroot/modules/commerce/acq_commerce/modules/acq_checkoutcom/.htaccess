# Deny all requests from Apache 2.4+.
<FilesMatch "\.(csv)$">
  <IfModule mod_authz_core.c>
    Require all denied
  </IfModule>
  # Deny all requests from Apache 2.0-2.2.
  <IfModule !mod_authz_core.c>
    Deny from all
  </IfModule>
</FilesMatch>
# Turn off all options we don't need.
Options -Indexes -ExecCGI -Includes -MultiViews
