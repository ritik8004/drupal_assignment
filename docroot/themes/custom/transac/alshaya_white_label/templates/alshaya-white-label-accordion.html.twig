<div class="{{ title|clean_class }} c-accordion-delivery-options">
  <h3 class="c-accordion__title">
    {% if title %}
      {{ title }}
    {% endif %}
  </h3>
  {% if content is not empty %}
  <div class="c-accordion_content">
    <ul>
    {% for term in content %}
        <li>
          <a href="{{ term.path }}" class="{{ term.active_class }}">{{ term.label }}</a>
        </li>
    {% endfor %}
    </ul>
  </div>
  {% endif %}
</div>
