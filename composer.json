{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "HubSpot/pace": "^1.0",
        "acquia/acsf-tools": "dev-9.x-dev",
        "acquia/blt": "^9.0.0",
        "acquia/http-hmac-php": "~3.2",
        "acquia/lightning": "^3.0.0",
        "charliekassel/array-diff": "^0.0.2",
        "ckeditor/font": "^4.9",
        "claviska/jquery-minicolors": "^2.2",
        "drupal/acquia_connector": "^1.9.0",
        "drupal/acquia_purge": "^1.0@beta",
        "drupal/acsf": "^2.47.0",
        "drupal/address": "^1.0-rc4",
