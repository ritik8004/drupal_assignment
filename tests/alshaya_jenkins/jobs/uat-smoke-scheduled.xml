<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Runs once per hour</description>
  <displayName>UAT - Scheduled</displayName>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>H 1 * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cd /behat
        echo Building
        ./behat-build.sh --rebuild=TRUE --site=vs-ae-uat-en
        echo Running tests
        bin/behat --profile=vs-ae-uat-en-desktop --tags=&quot;@smoke&quot;
      </command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>
