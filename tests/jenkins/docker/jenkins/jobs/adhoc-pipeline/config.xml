              <string>nb</string>
              <string>pb</string>
              <string>pbk</string>
              <string>tb</string>
              <string>ve</string>
              <string>vs</string>
              <string>we</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>Market</name>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>ae</string>
